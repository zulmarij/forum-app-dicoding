{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "react-app"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "react-hooks", "cypress"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "linebreak-style": ["error", "windows"],
    "no-alert": "off"
    
  }
}
